<%= form_for :user, url: '/users' do |f| %>
    <p>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :email %>
        <%= f.text_field :email %>
    </p>
    <p>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </p>
    <p>
        <%= f.label :verify_password %>    
        <%= f.password_field :verify_password %> 
    </p>
    <p>
        <%= f.label :Address %>
    </p>
        <p>
            <%= f.label :street_address %>
            <%= f.text_field :street_address %>
        </p>
        <p>
            <%= f.label :city %>
            <%= f.text_field :city %>
            <%= f.label :state %>
            <%= f.text_field :state %>
        </p>
        <p>
            <%= f.label :zip_code %>
            <%= f.text_field :zip %>
        </p>
    <p>
        <%= f.label :phone_number %>
        <%= f.text_field :phone_number %>
    </p>

<%end%>
