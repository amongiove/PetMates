    <%= f.error_notification %>

    <%= f.input :name, readonly: true %>
    <%= f.input :email, readonly: true %><br>

    Change profile picture:<br>
    <%if @user.image.attached?%>
        <%= image_tag @user.image, size: "100x100"%>
    <%else%>
        <%= image_tag ("default_user_image.png"), size: "100x100" %>
    <%end%><br>
    <%= f.file_field :image %><br>


    <br>Address:
        <%= f.input :street_address, input_html: { class: "form-control", required: true } %>
        <%= f.input :city, input_html: { class: "form-control", required: true } %>       
        <%= f.input :state, collection: us_states, prompt: :select, input_html: { class: "form-control", required: true }%>
        <%= f.input :zip, input_html: { class: "form-control", required: true } %>

    <%= f.input :phone_number, input_html: { class: "form-control", required: true } %>
        