  <%= simple_form_for :user, url: '/users' do |f| %>
    <%= f.error_notification %>

    <%= f.input :name %>
    <%= f.input :email, input_html: { autocomplete: 'email' } %>
    <%= f.input :password, input_html: { autocomplete: 'current-password' } %>
    <%= f.input :verify_password %>

    <br>Address:
        <%= f.input :street_address %>
        <%= f.input :city %>       
        <%= f.input :state, collection: us_states, prompt: :select%>
        <%= f.input :zip %>

    <%= f.input :phone_number %>
<%end%>
        